<!doctype html>
<html lang="fr">
  <head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta author="BECQUAERT Rémi">
    <link rel="icon" type="image/x-icon" href="images/logo.png">
    
   {% block stylesheets %}

    <link href="css/main.min.css" rel="stylesheet">
    <link href="css/style.css" rel="stylesheet">
     
   {% endblock %}
    <title>{% block title %}Cactus Esport - {% endblock %}</title>
  </head>
  <body>

<nav class="navbar navbar-expand-lg navbar-dark bg-dark">
  <div class="container d-flex">
      <a class="navbar-brand" href="{{path('index')}}">
        <img src="images/logo.png" width="45" height="50" alt="Logo Cactus Esport">
      </a>
    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarColor01" aria-controls="navbarColor01" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>

    <div class="collapse navbar-collapse" id="navbarColor01">
      <ul class="navbar-nav">
        <li class="nav-item dropdown">
          <a class="nav-link dropdown-toggle text-light" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false">
            Nos équipes
          </a>
          <ul class="dropdown-menu">
            <li><a class="dropdown-item" href="{{path('rainbow-six')}}">Rainbow Six Siege</a></li>
            <li><a class="dropdown-item" href="{{path('master-duel')}}">Master Duel</a></li>
            <li><a class="dropdown-item" href="{{path('league-of-legends')}}">League of Legends</a></li>
          </ul>
        </li>
        <li class="nav-item">
          <a class="nav-link active" href="{{path('shop')}}">Shop</a>
        </li>
        <li class="nav-item">
          <a class="nav-link active" href="{{path('app_article_index')}}">Actualités</a>
        </li>
        <li class="nav-item">
          <a class="nav-link active" href="{{path('about')}}">Staff</a>
        </li>
        <li class="nav-item">
          <a class="nav-link active" href="{{path('contact')}}">Contact</a>
        </li>
        {% if is_granted('IS_AUTHENTICATED_FULLY') %}
        <li class="nav-item dropdown">
          <a class="nav-link dropdown-toggle text-light" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false">
            Contrôles
          </a>
          <ul class="dropdown-menu">
            <li><a class="dropdown-item" href="{{path('liste-contact')}}">Liste des contacts</a></li>
            <div class="dropdown-divider"></div>
            <li><a class="dropdown-item" href="{{path('app_article_new')}}">Créer un article</a></li>
            <li><a class="dropdown-item" href="{{path('app_create_sujet')}}">Ajouter un sujet d'article</a></li>

          </ul>
        </li>
        {% endif %}
      </ul>
    </div>
        <ul class="nav navbar-nav navbar-right">
          {% if not app.user %}
            <a class="btn btn-outline-primary" href="{{path('app_login')}}">Se connecter</a>
            <a class="btn btn-outline-primary" href="{{path('app_register')}}">S'inscrire</a>
          {% endif %}
          {% if app.user %}
            <a class="btn btn-outline-primary" href="{{path('app_logout')}}">Se déconnecter</a>
          {% endif %}
        </ul>
  </div>
</nav>

    {% for message in app.flashes('notice') %}
     <h2 class="alert alert-warning text-center" role="alert">
      {{ message }}
      </h2>
    {% endfor %}

    {% for message in app.flashes('danger') %}
     <h2 class="alert alert-danger text-center" role="alert">
      {{ message }}
      </h2>
    {% endfor %}


